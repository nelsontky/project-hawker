{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll",
    "deploy": "git add -f out/ && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./ormconfig.ts",
    "typeorm:create": "npm run typeorm migration:create -- -n",
    "typeorm:generate": "npm run typeorm migration:generate -- -n",
    "typeorm:migrate": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "clsx": "^1.1.1",
    "date-fns": "^2.11.1",
    "gray-matter": "^4.0.2",
    "lodash": "^4.17.21",
    "next": "^10.0.0",
    "pg": "^8.6.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "reflect-metadata": "^0.1.13",
    "remark": "^13.0.0",
    "remark-html": "^12.0.0",
    "sharp": "^0.28.3",
    "slugify": "^1.5.3",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@types/lodash": "^4.14.170",
    "@types/node": "^13.13.52",
    "@types/react": "^16.9.32",
    "@types/sharp": "^0.28.2",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.3.0",
    "tailwindcss": "^2.1.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
